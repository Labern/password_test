<h1>All Users</h1>

<%= link_to "Sign Up", new_user_path %>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        <% @users.each do |user| %>
            <tr>
                <td><%= user.id %></td>
                <td><%= user.name %></td>
            </tr>
        <% end %>
    </tbody>
</table>